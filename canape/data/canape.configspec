[xmlrpc]
start = boolean(default=True)
hostname = string(default=localhost)
port = integer(default=8080)

[tvshow]
quality = string(default=None)
subtitles = string(max=2,default=None)
check_interval = integer(default=60)
check_timedelta = integer(default=36)

[sources]
video = force_list(default=tpb)
subtitle = force_list(default=tvsubtitles)

[downloader]
download_dir = string(default=~/canapedl)
